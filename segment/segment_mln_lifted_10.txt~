#domains
citation={0, ..., 10}
position={0, ..., 10}
token={0, ..., 10}
field={0, ..., 10}

#predicates
Token(token, position, citation)
InField(position, field, citation)
SameField(field, citation, citation)
SameCitation(citation, citation)
Equal(field, field)

#formulas
( !Token(t, i, c)   |  InField(i, f, c)) ::0.54657
( !InField(i, f, c) |  InField(i, f, c)) ::1.68255
(  InField(i, f, c) | !InField(i, f, c)) ::0.79176
(  Equal(f1, f2)    | !InField(i, f1, c) | !InField(i, f2, c)) ::1.95163
(  Token(token, position, citation)) ::1.06268
( !InField(position, field, citation)) ::1.78822
( !Equal(f, f)) ::0.70025
